    <ui:LogMessage DisplayName="Log Message" sap:VirtualizedContainerService.HintSize="450,173" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Info" Message="[&quot;Worksheet_ChangeRangeColor: START&quot;]" />
    <ui:InvokeCode ContinueOnError="{x:Null}" Code="Try&#xD;&#xA;    ' Load the Excel file using ClosedXML&#xD;&#xA;    Using workbook As New ClosedXML.Excel.XLWorkbook(excelFilePath)&#xD;&#xA;        Console.WriteLine(&quot;Loaded Excel file: &quot; &amp; excelFilePath)&#xD;&#xA;&#xD;&#xA;        ' Get the specified worksheet&#xD;&#xA;        Dim worksheet As ClosedXML.Excel.IXLWorksheet = workbook.Worksheet(WorksheetName)&#xD;&#xA;        Console.WriteLine(&quot;Retrieved worksheet: &quot; &amp; WorksheetName)&#xD;&#xA;&#xD;&#xA;        ' Get the specified range&#xD;&#xA;        Dim range As ClosedXML.Excel.IXLRange = worksheet.Range(ColorRange)&#xD;&#xA;        Console.WriteLine(&quot;Selected range: &quot; &amp; ColorRange)&#xD;&#xA;&#xD;&#xA;        ' Choose color below after Color.&#xD;&#xA;        Dim color As System.Drawing.Color = System.Drawing.Color.FromName(ColorName)&#xD;&#xA;        Console.WriteLine(&quot;Chosen color: &quot; &amp; ColorName)&#xD;&#xA;&#xD;&#xA;        ' Apply color to range&#xD;&#xA;        range.Style.Fill.BackgroundColor = ClosedXML.Excel.XLColor.FromColor(color)&#xD;&#xA;        Console.WriteLine(&quot;Applied color to range.&quot;)&#xD;&#xA;&#xD;&#xA;        ' Save the workbook&#xD;&#xA;        workbook.Save()&#xD;&#xA;&#xD;&#xA;        Console.WriteLine(&quot;Saved workbook: &quot; &amp; excelFilePath)&#xD;&#xA;    End Using&#xD;&#xA;Catch ex As Exception&#xD;&#xA;    Console.WriteLine(&quot;Error: &quot; &amp; ex.Message)&#xD;&#xA;End Try" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="450,277" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
