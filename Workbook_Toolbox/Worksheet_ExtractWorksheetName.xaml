    <ui:InvokeCode Code="Try&#xA;    Using workbook As New XLWorkbook(workbookPath)&#xA;        Dim worksheets As IXLWorksheets = workbook.Worksheets&#xA;        Dim worksheetCount As Integer = worksheets.Count&#xA;        Dim worksheetNameList As New List(Of String)()&#xA;        WsCount = worksheetCount&#xA;&#xA;        If worksheetCount = 1 Then&#xA;            ' Only one worksheet, extract its name&#xA;            WsName = worksheets(0).Name&#xA;        ElseIf worksheetCount &gt; 1 Then&#xA;            For Each worksheet As IXLWorksheet In worksheets&#xA;                worksheetNameList.Add(worksheet.Name)&#xA;            Next&#xA;            WsNameList = worksheetNameList&#xA;        Else&#xA;            Console.WriteLine(&quot;No worksheets found in the workbook.&quot;)&#xA;        End If&#xA;    End Using&#xA;Catch ex As Exception&#xA;    Console.WriteLine(&quot;Error: &quot; &amp; ex.Message)&#xA;End Try" ContinueOnError="False" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="450,277" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
